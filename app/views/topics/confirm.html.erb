<h3>以下の内容で、送信する。</h3>

<%= form_for(@topic) do |f| %>

  <%= f.hidden_field :title %>
  <%= f.hidden_field :content %>
  <%= f.hidden_field :image_cache, class: 'form-control'  %>

  <p>タイトル:<%= @topic.title %></p>
  <p>本文:<%= @topic.content %></p>
  <p>イメージ:<%= @topic.image %></p>
  <%= f.submit "登録する" %>
<% end %>
